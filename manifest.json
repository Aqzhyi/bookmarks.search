{
  "name": "Bookmarks search",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "Chrome extension for bookmarks search, the google way.",
  "homepage_url": "https://github.com/Pleasurazy/bookmarks.search",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "js": [
        "./dist/bg/node_modules/jquery/dist/jquery.min.js",
        "./dist/bg/index.js"
      ],
      "css": [
        "./dist/bg/index.css"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "background": {
    "page": "dist/bg/background.html",
    "persistent": true
  },
  "permissions": [
    "tabs",
    "activeTab",
    "bookmarks",
    "history",
    "http://*/*",
    "https://*/*"
  ]
}
